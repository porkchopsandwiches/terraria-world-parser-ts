{
	"name": "@porkchopsandwich/terraria-world-parser",
	"version": "0.0.5",
	"description": "Library for parsing Terraria worlds",
	"license": "MIT",
	"repository": "porkchopsandwiches/terraria-world-parser-ts",
	"author": {
		"name": "Cameron Morrow",
		"email": "cam.morrow+dev@gmail.com",
		"url": "https://github.com/porkchopsandwiches/terraria-world-parser-ts"
	},
	"type": "commonjs",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"build": "del-cli dist && tsc",
		"test:ava": "ava --timeout=2m",
		"test": "npm run lint && npm run test:ava && npm run build",
		"lint": "npx eslint src test",
		"lint:fix": "npx eslint src test --fix",
		"prepack": "npm run build"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"typescript",
		"terraria",
		"world"
	],
	"sideEffects": false,
	"devDependencies": {
		"@types/bytebuffer": "^5.0.43",
		"@types/ramda": "^0.28.13",
		"@typescript-eslint/eslint-plugin": "^5.27.1",
		"@typescript-eslint/parser": "^5.27.1",
		"ava": "^4.2.0",
		"bytebuffer": "^5.0.1",
		"del-cli": "^4.0.1",
		"dotenv": "^16.0.1",
		"eslint": "^8.17.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-unicorn": "^42.0.0",
		"npm-check-updates": "^12.5.11",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.4"
	},
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"dependencies": {
		"ramda": "^0.28.0"
	}
}
